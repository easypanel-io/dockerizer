{
  "name": "dockerizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'next dev' 'npm:compile-files-watch'",
    "build": "npm run compile && next build",
    "start": "next start",
    "lint": "next lint",
    "compile-files": "tsx src/scripts/compile-files.ts",
    "compile-files-watch": "nodemon --exec npm run compile-files --watch './src/dockerizers/*/files/**' --ext '*'",
    "compile-tests": "tsx src/scripts/compile-tests.ts",
    "compile": "npm run compile-files && npm run compile-tests",
    "move-dist": "rm -rf ../website/static/dockerizer/* && cp -r ./dist/* ../website/static/dockerizer/"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next/bundle-analyzer": "^14.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/nunjucks": "^3.2.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "file-saver": "^2.0.5",
    "glob": "^10.3.10",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.307.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "nodemon": "^3.0.2",
    "nunjucks": "^3.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "react-syntax-highlighter": "^15.5.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.11",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
